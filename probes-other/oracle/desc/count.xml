<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
    <name>OracleGaugeGraph</name>
    <graphName>${probename}</graphName>
    <graphTitle>DB Open on ${url}</graphTitle>
    <verticalLabel>value</verticalLabel>
    <add>
        <name>logonscurr</name>
        <graphType>line</graphType>
        <legend>logons current</legend>
    </add>
    <add>
        <name>opcurscurr</name>
        <graphType>line</graphType>
        <legend>opened cursors current</legend>
    </add>
    <hosttree>
        <pathstring>${host}</pathstring>
        <pathstring>Services</pathstring>
        <pathstring>Databases</pathstring>
        <pathstring>${url}</pathstring>
        <pathstring>DB Open</pathstring>
    </hosttree>
    <viewtree>
        <pathstring>Services</pathstring>
        <pathstring>Databases</pathstring>
        <pathstring>${url}</pathstring>
        <pathstring>DB Open</pathstring>
    </viewtree>
</graphdesc>